{
  "author": "Girma Nigusse <xgirma@gmail.com>",
  "bugs": {
    "url": "https://github.com/e2e-boilerplate/cypress-typescript-cucumber-chai-should/issues"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "dependencies": {
    "cucumber": "6.0.7",
    "cypress": "5.6.0",
    "cypress-cucumber-preprocessor": "3.0.0",
    "rimraf": "3.0.2",
    "ts-loader": "9.3.0",
    "tsify": "5.0.4",
    "typescript": "4.6.4"
  },
  "description": "Cypress end-to-end test automation boilerplate. Using TypeScript, Cucumber, Chai and Should.",
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/cypress-cucumber-preprocessor": "4.0.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.6.2",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0"
  },
  "homepage": "https://github.com/e2e-boilerplate/cypress-typescript-cucumber-chai-should#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "keywords": [
    "chai",
    "cucumber",
    "cypress",
    "cypress automation",
    "cypress boilerplate",
    "cypress example",
    "should",
    "typescript",
    "e2e",
    "e2e tests",
    "boilerplate",
    "integration test",
    "test automation",
    "javascript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "tslint --fix",
      "prettier --write"
    ]
  },
  "name": "cypress-typescript-cucumber-chai-should",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplate/cypress-typescript-cucumber-chai-should.git"
  },
  "scripts": {
    "clean:screenshots": "npx cypress/screenshots/*",
    "clean:video": "npx rimraf cypress/videos/*",
    "lint": "npx tslint -c tslint.json './cypress/**/*.ts'",
    "open": "npx cypress open",
    "pretty": "npx prettier --no-config './**/*.{ts,js}' --write",
    "test": "npx cypress run --spec **/*.feature"
  },
  "version": "1.0.0"
}